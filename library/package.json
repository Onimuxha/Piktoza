{
  "name": "piktoza",
  "version": "1.0.0",
  "description": "Modern icon library for React with 3000+ icons in 5 variants",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "generate": "node scripts/generate-components.js",
    "build": "pnpm run generate && rollup -c",
    "prepublishOnly": "pnpm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": ["react", "icons", "svg", "components", "typescript", "ui", "piktoza", "icon-library", "react-icons"],
  "author": "Piktoza Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/piktoza/piktoza.git"
  },
  "bugs": {
    "url": "https://github.com/piktoza/piktoza/issues"
  },
  "homepage": "https://piktoza.com",
  "packageManager": "pnpm@9.0.0",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "jest": "^29.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "svgo": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
